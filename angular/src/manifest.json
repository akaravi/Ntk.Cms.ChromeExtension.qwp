{
  "manifest_version": 2,
  "name": "qwp",
  "short_name": "qwp",
  "version": "1.0.0",
  "description": "quick website page",
  "permissions": [
   
  ],
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "index.html?#/popup",
    "default_title": "qwp Set And Get Short Link"
  },
  "options_page": "index.html?#/options",
  "content_scripts": [
    {
      "js": [
        "contentPage.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "scripts": [
      "backgroundPage.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
